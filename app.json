{
  "expo": {
    "name": "Little Lemon",
    "slug": "Little Lemon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LittleLemonIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LittleLemonLogoBig.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Give Little Lemon permission to save photos",
        "NSCameraUsageDescription": "Give Little Lemon permission to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LittleLemonLogoBig.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/LittleLemonIcon.png"
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ]
  }
}
